<h1 class="display-4 m-5">Celebrities</h1>
<div class="container m-5">
  <div class="row">
    <div class="col">


      {{#each celebrities}}
      <div class="card m-3">
        <div class="card-header">
          {{ this.name}}
          <a href="/celebrities/{{this._id}}" class="btn btn-primary float-right">View</a>
        </div>
      </div>
      {{/each}}



    </div>
  </div>
</div>